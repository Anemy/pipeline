    "classnames": "^2.2.6",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "d3": "^3.5.17",
    "detect-coordinates": "^0.2.0",
    "dotenv": "^8.2.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-is-dev": "^1.2.0",
    "jquery": "^3.5.1",
    "leaflet": "^1.7.1",
    "leaflet-defaulticon-compatibility": "^0.1.1",
    "leaflet-draw": "^1.0.4",
    "litegraph.js": "^0.7.8",
    "lodash": "^4.17.20",
    "mapbox-gl": "^1.12.0",
    "mongodb": "^3.6.2",
    "mongodb-client-encryption": "^1.1.0",
    "mongodb-query-util": "^0.2.1",
    "mongodb-schema": "^8.2.5",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.1",
    "numeral": "^2.0.6",
    "pluralize": "^8.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.8.0",
    "react-leaflet-draw": "^0.19.0",
    "react-redux": "^7.2.1",
    "react-splitter-layout": "^4.0.0",
    "react-tooltip": "^4.2.10",
    "redux": "^4.0.5",
    "smalltalk": "^4.0.7",
    "turf-destination": "^3.0.12",
    "turf-distance": "^3.0.12",
    "turf-point": "^2.0.1",
    "uuid": "^8.3.1",
    "wait-on": "^5.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
